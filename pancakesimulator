ImageLabel.Frame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)


		TopBar.MouseEnter:Connect(function()
			Frame.Active = true
			Frame.Selectable = true
			Frame.Draggable = true
		end)

		TopBar.MouseLeave:Connect(function()
			Frame.Active = false
			Frame.Selectable = false
			Frame.Draggable = false
		end)

		local au0 = 0

		Close.MouseButton1Click:Connect(function()
			if au0 == 0 then
				au0 = 2
				Close.ImageColor3 = Color3.fromRGB(255, 48, 48)								 
				ImageLabel.Frame:TweenSize(UDim2.new(1,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
				wait(0.2)
				ImageLabel:TweenSize(UDim2.new(1,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
				au0 = 1
			elseif au0 == 1 then
				au0 = 2
				Close.ImageColor3 = Color3.fromRGB(66, 255, 33)
				ImageLabel:TweenSize(UDim2.new(1,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
				wait(0.2)
				ImageLabel.Frame:TweenSize(UDim2.new(1,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
				au0 = 0
			end
		end)


		----------------------------------------------------------------



		function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
		counter = 0

		spawn(function()
			while wait(0.1)do
				Rainbow.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
				counter = counter + 0.01
			end
		end)


		for i, v in pairs(ImageLabel:GetChildren()) do
			if v:isA("ImageButton") then
				v.MouseEnter:Connect(function()
					v.ImageColor3 = Color3.fromRGB(34,34,34)
				end)
				v.MouseLeave:Connect(function()
					v.ImageColor3 = Color3.fromRGB(26,26,26)
				end)
			end
		end


		local au2 = false

		---------------------------------------

		-- Auto Farm Coins


		AutoFarmCoins.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if au2 == false then
					au2 = true
					TextLabel_2.Text = "Auto Farm Coins : ON"
				elseif au2 == true then
					au2 = false
					TextLabel_2.Text = "Auto Farm Coins : OFF"
				end
			end
		end)

		local au1 = false

		--------------------------------

		-- Auto Farm Pancakes

		AutoFarmPancakes.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if au1 == false then
					au1 = true
					TextLabel.Text = "Auto Farm Pancake : ON"
					for i, v in pairs(player.Backpack:GetChildren()) do
						if v:isA("Tool") then
							v.Parent = player.Character
						end
					end
				elseif au1 == true then
					au1 = false
					TextLabel.Text = "Auto Farm Pancake : OFF"
				end
			end
		end)

		-------------------------------------


		spawn(function()
			while true do
				wait(0.1)
				if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
					if au2 == true then
						for i, v in pairs(game.Workspace.Coins:GetChildren()) do
							wait()
							v.CFrame = player.Character.HumanoidRootPart.CFrame
						end
					end
				end
			end
		end)

		spawn(function()
			while true do
				wait(0.1)
				if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
					if au1 == true then
						for i, v in pairs(player.Backpack:GetChildren()) do
							if v:isA("Tool") then
								v.Parent = player.Character
							end
						end
						game.Players.LocalPlayer.Character.Pancake.E:FireServer()
					end
				end
			end
		end)

		CollectAllCoins.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				for i, v in pairs(game.Workspace.Coins:GetChildren()) do
					wait()
					v.CFrame = player.Character.HumanoidRootPart.CFrame
				end
			end
		end)

		-------------------------------------------

		local speed = false
		local jump = false
		local s1 = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
		local j1 =  game.Players.LocalPlayer.Character.Humanoid.JumpPower

		SuperSpeed.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if speed == false then
					speed = true 
					TextLabel_3.Text = "Super Speed : ON"
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
				elseif speed == true then
					speed = false
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s1
					TextLabel_3.Text = "Super Speed : OFF"
				end
			end
		end)

		JumpHack.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if jump == false then
					jump = true 
					TextLabel_4.Text = "Jump Hack : ON"
					game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
				elseif jump == true then
					jump = false
					game.Players.LocalPlayer.Character.Humanoid.JumpPower = j1
					TextLabel_4.Text = "Jump Hack : OFF"
				end
			end
		end)


		----------------------------------

		-- Auto Sell 

		local as = game.Workspace["Sell For Coins"].Sell.CFrame

		local au7 = false

		AutoSell.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if au7 == false then
					au7 = true
					TextLabel_7.Text = "Auto Sell : ON"
				elseif au7 == true then
					au7 = false
					TextLabel_7.Text = "Auto Sell : OFF"
					game.Workspace["Sell For Coins"].Sell.CFrame = as
				end
			end
		end)


		spawn(function()
			while true do
				wait(0.1)
				if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
					if au7 == true then
						game.Workspace["Sell For Coins"].Sell.CFrame = player.Character.HumanoidRootPart.CFrame
						wait()
						game.Workspace["Sell For Coins"].Sell.CFrame = CFrame.new(1000,1000,1000)
					end
				end
			end
		end)

		------------------------------------

		-- Auto Buy all 

		local au6 = false

		AutoBuyAll.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if au6 == false then
					au6 = true
					TextLabel_5.Text = "Auto Buy All : ON"
				elseif au6 == true then
					au6 = false
					TextLabel_5.Text = "Auto Buy All : OFF"
				end
			end
		end)

		spawn(function()
			while true do
				wait(0.1)
				if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
					if au6 == true then
						for i = 1, 30 do
							game:GetService("ReplicatedStorage").Events.UnlockFlavor:FireServer(i)
							game:GetService("ReplicatedStorage").Events.UnlockPlate:FireServer(i)
							game:GetService("ReplicatedStorage").Events.UnlockRank:FireServer(i)
						end
					end
				end
			end
		end)

		----------------------------------------

		------------------------------------

		-- Infinite Coins

		local au16 = false

		InfiniteCoins.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if au16 == false then
					au16 = true
					TextLabel_8.Text = "Infinite Coins : ON"
					for i, v in pairs(player.Backpack:GetChildren()) do
						if v:isA("Tool") then
							v.Parent = player.Character
						end
					end
				elseif au16 == true then
					au16 = false
					TextLabel_8.Text = "Infinite Coins : OFF"
					game.Workspace["Sell For Coins"].Sell.CFrame = as

				end
			end
		end)

		spawn(function()
			while true do
				wait()
				if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
					if au16 == true then
						for i, v in pairs(player.Backpack:GetChildren()) do
							if v:isA("Tool") then
								v.Parent = player.Character
							end
						end
						for i = 0,1,0.01 do
							game.Players.LocalPlayer.Character.Pancake.E:FireServer()
						end
						game.Workspace["Sell For Coins"].Sell.CFrame = player.Character.HumanoidRootPart.CFrame
						wait()
						game.Workspace["Sell For Coins"].Sell.CFrame = CFrame.new(1000,1000,1000)

					end
				end
			end
		end)



		----------------------------------------

		FillPancakeStorage.MouseButton1Click:Connect(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				for i, v in pairs(player.Backpack:GetChildren()) do
					if v:isA("Tool") then
						v.Parent = player.Character
					end
				end
				for i = 0,1,0.01 do
					game.Players.LocalPlayer.Character.Pancake.E:FireServer()
				end
			end
		end)


		-------------------------------------------------

	end
