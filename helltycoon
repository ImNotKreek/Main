	local au0 = 0
	local s1 = ImageLabel.Size
	Close.MouseButton1Click:Connect(function()
		if au0 == 0 then
			au0 = 2
			Close.ImageColor3 = Color3.fromRGB(255, 48, 48)                              
			ImageLabel.Frame:TweenSize(UDim2.new(1,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
			wait(0.2)
			ImageLabel:TweenSize(UDim2.new(1,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
			au0 = 1
		elseif au0 == 1 then
			au0 = 2
			Close.ImageColor3 = Color3.fromRGB(66, 255, 33)
			ImageLabel:TweenSize(s1,Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
			wait(0.2)
			ImageLabel.Frame:TweenSize(UDim2.new(1,0,0,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.2,true,nil)
			au0 = 0
		end
	end)


	----------------------------------------------------------------



	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	counter = 0

	spawn(function()
		while wait(0.1)do
			Rainbow.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
			counter = counter + 0.01
		end
	end)


	for i, v in pairs(ImageLabel:GetChildren()) do
		if v:isA("ImageButton") then
			v.MouseEnter:Connect(function()
				v.ImageColor3 = Color3.fromRGB(34,34,34)
			end)
			v.MouseLeave:Connect(function()
				v.ImageColor3 = Color3.fromRGB(26,26,26)
			end)
		end
	end


	-------------------------------------------

	local speed = false
	local jump = false
	local s1 = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed
	local j1 =  game.Players.LocalPlayer.Character.Humanoid.JumpPower

	SuperSpeed.MouseButton1Click:Connect(function()
		local success,err = pcall(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if speed == false then
					speed = true 
					Text_SuperSpeed.Text = "Super Speed : ON"
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
				elseif speed == true then
					speed = false
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s1
					Text_SuperSpeed.Text = "Super Speed : OFF"
				end
			end
		end)
	end)

	JumpHack.MouseButton1Click:Connect(function()
		local success,err = pcall(function()
			if game.Players.LocalPlayer.Character.Humanoid.Health ~= 0 then
				if jump == false then
					jump = true 
					Text_JumpHack.Text = "Jump Hack : ON"
					game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
				elseif jump == true then
					jump = false
					game.Players.LocalPlayer.Character.Humanoid.JumpPower = j1
					Text_JumpHack.Text = "Jump Hack : OFF"
				end
			end
		end)
	end)

	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			local success,err = pcall(function()
				if speed == true then
					game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
				end
			end)
		end)
	end)

	spawn(function()
		game:GetService("RunService").RenderStepped:Connect(function()
			local success,err = pcall(function()
				if speed == true then
					game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
				end
			end)
		end)
	end)


	----------------------------------

	KillAll.MouseButton1Click:Connect(function()
		local success,err = pcall(function()
			for i, v in pairs(game.Players:GetChildren()) do
				if v.Name ~= game.Players.LocalPlayer.Name then
					if v.Character.Humanoid.Health ~= 0 then
						game.ReplicatedStorage.Events.destryObj:FireServer(v.Character.Torso.Neck)
					end
				end
			end
		end)
	end)

	local au1 = false

	KillAura.MouseButton1Click:Connect(function()
		if au1 == false then
			au1 = true
			Text_KillAura.Text = "Kill Aura : ON"
		elseif au1 == true then
			au1 = false
			Text_KillAura.Text = "Kill Aura : OFF"
		end
	end)


	spawn(function()
		while true do
			wait(0.1)
			if au1 == true then
				local success,err = pcall(function()
					for i, v in pairs(game.Players:GetChildren()) do
						if v.Name ~= game.Players.LocalPlayer.Name then
							if v.Character.Humanoid.Health ~= 0 then
								if (v.Character.HumanoidRootPart.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude < 50 then
									game.ReplicatedStorage.Events.destryObj:FireServer(v.Character.Torso.Neck)
								end
							end
						end
					end
				end)
			else

			end
		end
	end)

	---------------------------------------------------------------------------

	KickAll.MouseButton1Click:Connect(function()
		local success,err = pcall(function()
			for i, v in pairs(game.Players:GetChildren()) do
				if v.Name ~= game.Players.LocalPlayer.Name then
					game.ReplicatedStorage.Events.destryObj:FireServer(v)
				end
			end
		end)
	end)

