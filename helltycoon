-- INFINITE AIRSTRIKE

local INF_AIRSTRIKE = false

InfiniteAirstrike.MouseButton1Click:Connect(function()
	if INF_AIRSTRIKE == false then
		INF_AIRSTRIKE = true
		InfiniteAirstrike.TextLabel.Text = "Infinite Airstrike : ON"
	elseif INF_AIRSTRIKE == true then
		INF_AIRSTRIKE = false
		InfiniteAirstrike.TextLabel.Text = "Infinite Airstrike : OFF"
	end
end)

mouse = game.Players.LocalPlayer:GetMouse()

mouse.Button1Down:Connect(function()
	if INF_AIRSTRIKE == true then
	local success,err = pcall(function()
		game.Players.LocalPlayer.Character["Tactical Airstrike"].OnMouseClick:FireServer(mouse.Hit.p)
		wait()
		spawn(function()
			for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
				if v.Name == "Tactical Airstrike" then
					v.Parent = game.Lighting
					wait(18)
					game.ReplicatedStorage.Events.destryObj:FireServer(v)
				end
			end
		end)
		spawn(function()
			for i, v in pairs(game.Lighting:GetChildren()) do
				if v.Name == "AdminTools" then
					local success,err = pcall(function()
						game.ReplicatedStorage.Events.CrateClaim:FireServer("Gear",v["Tactical Airstrike"])
						wait()
					end)
				end
			end
		end)
		spawn(function()
			wait(0.2)
			for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				if v.Name == "Tactical Airstrike" then
					print("ok")
					v.Parent = game.Players.LocalPlayer.Character
					break
				end
			end
		end)

		end)
	end
end)
