Top.MouseEnter:Connect(function()
	Frame.Active = true
	Frame.Selectable = true
	Frame.Draggable = true
end)

Top.MouseLeave:Connect(function()
	Frame.Active = false
	Frame.Selectable = false
	Frame.Draggable = false
end)

local debounce = false


Close.MouseButton1Click:Connect(function()
	if debounce == false then
		debounce = true
		Close.BackgroundColor3 = Color3.fromRGB(235, 63, 63)
		Main.Visible = false
	elseif debounce == true then
		debounce = false
		Close.BackgroundColor3 = Color3.fromRGB(73, 235, 24)
		Main.Visible = true
	end
end)

function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
counter = 0


spawn(function()
	while wait(0.05)do
		Title.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.01
	end
end)

-----------------------------
-------------------------------------------------------------------------------------------------------


-- More Variables


local player = game.Players.LocalPlayer


local WHITELIST_FRIENDS = false
local KILL_AURA = false
local KILL_AURA_RANGE = 50

---------------------------------------------------------------


local speed = false
local jump = false
local s1 = player.Character.Humanoid.WalkSpeed
local j1 =  player.Character.Humanoid.JumpPower

SuperSpeed.MouseButton1Click:Connect(function()
	local success,err = pcall(function()
		if player.Character.Humanoid.Health ~= 0 then
			if speed == false then
				speed = true 
				TextLabel.Text = "Super Speed : ON"
				player.Character.Humanoid.WalkSpeed = 100
			elseif speed == true then
				speed = false
				player.Character.Humanoid.WalkSpeed = s1
				TextLabel.Text = "Super Speed : OFF"
			end
		end
	end)
end)

JumpHack.MouseButton1Click:Connect(function()
	local success,err = pcall(function()
		if player.Character.Humanoid.Health ~= 0 then
			if jump == false then
				jump = true 
				TextLabel_2.Text = "Jump Hack : ON"
				player.Character.Humanoid.JumpPower = 200
			elseif jump == true then
				jump = false
				player.Character.Humanoid.JumpPower = j1
				TextLabel_2.Text = "Jump Hack : OFF"
			end
		end
	end)
end)


---------------------

-- WEAPONS BUTTON

local WEAPONS_FRAME_VIS = false

WeaponsButton.MouseButton1Click:Connect(function()
	if WEAPONS_FRAME_VIS == false then
		WEAPONS_FRAME_VIS = true
		WeaponsFrame.Visible = true
		WeaponsButton.TextLabel.Text = "-"
	elseif WEAPONS_FRAME_VIS == true then
		WEAPONS_FRAME_VIS = false
		WeaponsFrame.Visible = false
		WeaponsButton.TextLabel.Text = "+"
	end
end)

-----------------------------

spawn(function()
	game:GetService("RunService").RenderStepped:Connect(function()
		local success,err = pcall(function()
			if speed == true then
				player.Character.Humanoid.WalkSpeed = 100
			end
		end)
	end)
end)

spawn(function()
	game:GetService("RunService").RenderStepped:Connect(function()
		local success,err = pcall(function()
			if jump == true then
				player.Character.Humanoid.JumpPower = 200
			end
		end)
	end)
end)



WhitelistFriends.MouseButton1Click:Connect(function()
	if WHITELIST_FRIENDS == false then
		WHITELIST_FRIENDS = true
		TextLabel_8.Text = "Whitelist Friends : ON"
	elseif WHITELIST_FRIENDS == true then
		WHITELIST_FRIENDS = false
		TextLabel_8.Text = "Whitelist Friends : OFF"
	end
end)

KillAll.MouseButton1Click:Connect(function()
	local success,err = pcall(function()
		for i, v in pairs(game.Players:GetChildren()) do
			--------------------------------------------------------------
			if WHITELIST_FRIENDS == false then
				if v.Name ~= game.Players.LocalPlayer.Name then
					if v.Character.Humanoid.Health ~= 0 then
						game.ReplicatedStorage.Events.destryObj:FireServer(v.Character.Torso.Neck)
					end
				end
			elseif WHITELIST_FRIENDS == true then
				if v.Name ~= game.Players.LocalPlayer.Name and not v:IsFriendsWith(game.Players.LocalPlayer.UserId) then
					if v.Character.Humanoid.Health ~= 0 then
						game.ReplicatedStorage.Events.destryObj:FireServer(v.Character.Torso.Neck)
					end
				end
			end
			--------------------------------------------------------------
		end
	end)
end)


-----------------------

KickAll.MouseButton1Click:Connect(function()
	local success,err = pcall(function()
		for i, v in pairs(game.Players:GetChildren()) do
			--------------------------------------------------------------
			if WHITELIST_FRIENDS == false then
				if v.Name ~= game.Players.LocalPlayer.Name then
					game.ReplicatedStorage.Events.destryObj:FireServer(v)
				end
			elseif WHITELIST_FRIENDS == true then
				if v.Name ~= game.Players.LocalPlayer.Name and not v:IsFriendsWith(game.Players.LocalPlayer.UserId) then
					game.ReplicatedStorage.Events.destryObj:FireServer(v)
				end
			end
			--------------------------------------------------------------
		end
	end)
end)

RagdollAll.MouseButton1Click:Connect(function()
	local success,err = pcall(function()
		for i, v in pairs(game.Players:GetChildren()) do
			--------------------------------------------------------------
			if WHITELIST_FRIENDS == false then
				if v.Name ~= game.Players.LocalPlayer.Name then
					game.ReplicatedStorage.Events.destryObj:FireServer(v.Character.HumanoidRootPart)
				end
			elseif WHITELIST_FRIENDS == true then
				if v.Name ~= game.Players.LocalPlayer.Name and not v:IsFriendsWith(game.Players.LocalPlayer.UserId) then
					game.ReplicatedStorage.Events.destryObj:FireServer(v.Character.HumanoidRootPart)
				end
			end
			--------------------------------------------------------------
		end
	end)
end)

