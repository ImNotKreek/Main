local cd = false

ClickDestroy.MouseButton1Click:Connect(function()
	if cd == false then
		cd = true
		ClickDestroy.TextLabel.Text = "Click Destroy : ON"
	elseif cd == true then
		cd = false
		ClickDestroy.TextLabel.Text = "Click Destroy : OFF"
	end
end)

local mouse = game.Players.LocalPlayer:GetMouse()

mouse.Button1Down:Connect(function()
	local success,err = pcall(function()
		if cd == true then
			print(mouse.Target.Name)
			if mouse.Target.Name ~= "Baseplate" and mouse.Target.Name ~= "baseplate" then
				game.ReplicatedStorage.Events.destryObj:FireServer(mouse.Target)
			else

			end
		end
	end)
end)


