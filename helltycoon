
if game.CoreGui:FindFirstChild("KreekHub") then
	game.StarterGui:SetCore("SendNotification", {
		Title = "Kreek Hub",
		Text = "Script already loaded!",
		Duration = 5,
	})

else

	-----------------------------------------------------------------------

	local fps = 60
	local clock = tick()

	spawn(function()
		while true do
			while clock + 1/fps > tick() do end
			wait()
			clock = tick()
		end
	end)

	game.StarterGui:SetCore("SendNotification", {
		Title = "Kreek Hub",
		Text = "Loading...",
		Duration = 5,
	}
	)

	wait(1)

	fps = 0.5

	wait(2)

	fps = 60

	game.StarterGui:SetCore("SendNotification", {
		Title = "Loaded Kreek Hub!",
		Text = "Made by ImNotKreek#0001",
		Duration = 5,
	}
	)


	----------------------------------------------------------------------------

	function GetPlayer(String)
		local Found = {}
		local strl = String:lower()
		if strl ~= "" then
			for i,v in pairs(game.Players:GetPlayers()) do
				if v.Name:lower():sub(1, #String) == String:lower() then
					table.insert(Found,v.Name)
				end
			end    
		end
		return Found    
	end


