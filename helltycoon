-- WEAPONS

AK47.MouseButton1Click:Connect(function()
	game.ReplicatedStorage.Events.CrateClaim:FireServer("Gear",game.ReplicatedStorage.Weapons["AK-47"])
end)

RocketLauncher.MouseButton1Click:Connect(function()
	game.ReplicatedStorage.Events.CrateClaim:FireServer("Gear",game.Lighting.BigRocketLauncher)
end)

BlackHole.MouseButton1Click:Connect(function()
	game.ReplicatedStorage.Events.CrateClaim:FireServer("Gear",game.ReplicatedStorage.Weapons["The Black Hole"])
end)

Airstrike.MouseButton1Click:Connect(function()
	for i, v in pairs(game.Lighting:GetChildren()) do
		if v.Name == "AdminTools" then
			local success,err = pcall(function()
				game.ReplicatedStorage.Events.CrateClaim:FireServer("Gear",v["Tactical Airstrike"])
			end)
		end
	end
end)

MagicCarpet.MouseButton1Click:Connect(function()
	game.ReplicatedStorage.Events.CrateClaim:FireServer("Gear",game.Lighting["RainbowMagicCarpet"])
end)

GiveAllCash.MouseButton1Click:Connect(function()
	for i,v in pairs(game.ReplicatedStorage.PlayerMoney:GetChildren()) do
		game.ReplicatedStorage.Events.changeValue:InvokeServer(v, "inf")
	end
end)

ResetAllCash.MouseButton1Click:Connect(function()
	for i,v in pairs(game.ReplicatedStorage.PlayerMoney:GetChildren()) do
		game.ReplicatedStorage.Events.changeValue:InvokeServer(v, 0)
	end
end)

local InfiniteJumpEnabled = false

InfiniteJump.MouseButton1Click:Connect(function()
	if InfiniteJumpEnabled == false then
		InfiniteJumpEnabled = true
		InfiniteJump.TextLabel.Text = "Infinite Jump : ON"
	elseif InfiniteJumpEnabled == true then
		InfiniteJumpEnabled = false
		InfiniteJump.TextLabel.Text = "Infinite Jump : OFF"
	end
end)

game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)

CrashServer.MouseButton1Click:Connect(function()
	for i, v in pairs(game.Players:GetChildren()) do
		game.ReplicatedStorage.Events.destryObj:FireServer(v)
	end
end)

FreezeAll.MouseButton1Click:Connect(function()
	local success,err = pcall(function()
		for i, v in pairs(game.Players:GetChildren()) do
			if v.Name ~= game.Players.LocalPlayer.Name and not v:IsFriendsWith(game.Players.LocalPlayer.UserId) then
				game.ReplicatedStorage.Events.destryObj:FireServer(v.Character.Humanoid) -- MAKE v.CHARACTER.HUMANOID IF YOU WANT TO RAGDOLL PERMANENTLY
			end
		end
	end)
end)

TextBox_6.FocusLost:Connect(function()
	local success,err = pcall(function()
		for i,v in pairs(GetPlayer(TextBox_6.Text)) do
			if game.Players[v] then 
				game.ReplicatedStorage.Events.destryObj:FireServer(game.Players[v].Character.Humanoid) -- MAKE v.CHARACTER.HUMANOID IF YOU WANT TO RAGDOLL PERMANENTLY
			else
			end
		end
	end)
end)

GiveCash.TextBox.FocusLost:Connect(function()
	local success,err = pcall(function()
		for i,v in pairs(game.ReplicatedStorage.PlayerMoney:GetChildren()) do
			if v.Name == game.Players.LocalPlayer.Name then

				game.ReplicatedStorage.Events.changeValue:InvokeServer(v, tonumber(GiveCash.TextBox.Text))

			else
			end
		end
	end)
end)

KillPlayer.TextBox.FocusLost:Connect(function()
	local success,err = pcall(function()
		for i,v in pairs(GetPlayer(KillPlayer.TextBox.Text)) do
			if game.Players[v].Character.Humanoid.Health ~= 0 then
				game.ReplicatedStorage.Events.destryObj:FireServer(game.Players[v].Character.Torso.Neck)
			end
		end
	end)
end)

NakedPlayer.TextBox.FocusLost:Connect(function()
	local success,err = pcall(function()
		for i, v in pairs(GetPlayer(NakedPlayer.TextBox.Text)) do
			if game.Players[v].Character.Humanoid.Health ~= 0 then
				if game.Players[v].Character.Humanoid.Health ~= 0 then
					if game.Players[v].Character:FindFirstChildOfClass("Shirt") then
						game.ReplicatedStorage.Events.destryObj:FireServer(game.Players[v].Character:FindFirstChildOfClass("Shirt"))
					end
					if game.Players[v].Character:FindFirstChildOfClass("Pants") then
						game.ReplicatedStorage.Events.destryObj:FireServer(game.Players[v].Character:FindFirstChildOfClass("Pants"))
					end
					if game.Players[v].Character:FindFirstChild("Shirt Graphic") then
						game.ReplicatedStorage.Events.destryObj:FireServer(game.Players[v].Character:FindFirstChildOfClass("Shirt Graphic"))
					end
				end
			end
		end
	end)
end)

KickPlayer.TextBox.FocusLost:Connect(function()
	local success,err = pcall(function()
		for i,v in pairs(GetPlayer(KickPlayer.TextBox.Text)) do
			if game.Players[v] then 
				game.ReplicatedStorage.Events.destryObj:FireServer(game.Players[v])
			else
			end
		end
	end)
end)

